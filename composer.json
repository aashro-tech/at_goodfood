{
    "name": "aashro/at-goodfood",
    "type": "typo3-cms-extension",
    "description": "Good Food is a modern and responsive TYPO3 template designed for restaurants, caf√©s, food blogs, and catering businesses. It features a clean and elegant design, easy customization, and SEO-friendly structure to showcase menus, recipes, and services beautifully.",
    "keywords": [
        "TYPO3 CMS",
        "bootstrap",
        "responsive",
        "frontend",
        "theme",
        "templates"
      ],
    "authors": [
        {
            "name": "Aashro Tech",
            "email": "info@aashro.com",
            "role": "Developer",
            "homepage": "http://www.aashro.com/"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5 || ^12.4",
        "mask/mask": "^8.3.11"
    },
    "autoload": {
        "psr-4": {
            "AASHRO\\GoodFood\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/at-goodfood": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "at_goodfood"
        }
    }
}